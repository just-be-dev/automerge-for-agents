{
  "name": "automerge-fs",
  "version": "0.3.0",
  "description": "A versionable filesystem for just-bash using Automerge CRDTs",
  "type": "module",
  "main": "dist/automerge-fs-v2.js",
  "types": "dist/automerge-fs-v2.d.ts",
  "exports": {
    ".": {
      "import": "./dist/automerge-fs-v2.js",
      "types": "./dist/automerge-fs-v2.d.ts"
    },
    "./blobs": {
      "import": "./dist/blob-stores.js",
      "types": "./dist/blob-stores.d.ts"
    },
    "./v1": {
      "import": "./dist/automerge-fs.js",
      "types": "./dist/automerge-fs.d.ts"
    }
  },
  "bin": {
    "automerge-fsd": "./src/daemon/main.ts",
    "amfs": "./src/cli/main.ts"
  },
  "scripts": {
    "build": "bun build ./src/daemon/main.ts --outfile=dist/daemon.js --target=bun && bun build ./src/cli/main.ts --outfile=dist/cli.js --target=bun",
    "dev": "bun run src/daemon/main.ts start",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@automerge/automerge": "^3.0.0",
    "@effect/ai": "^0.33.2",
    "@effect/cli": "^0.73.1",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/rpc": "^0.73.0",
    "effect": "^3.19.14"
  },
  "peerDependencies": {
    "just-bash": "^2.7.0"
  },
  "optionalDependencies": {
    "@automerge/automerge-repo": "^2.0.0",
    "@automerge/automerge-repo-storage-nodefs": "^2.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "oxlint": "^1.42.0",
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "@aspect-build/rules_ts"
  ],
  "private": true
}
